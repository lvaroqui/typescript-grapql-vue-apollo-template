{
  "name": "front",
  "version": "1.0.0",
  "description": "Frontend",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"vite --port 443 --https true\" \"yarn generate --watch\"",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier 'src/**/*.ts' 'src/**/*.vue' --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s test:*",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier --check 'src/**/*.ts' 'src/**/*.vue'",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "pinia": "^2.0.0-alpha.7",
    "vue": "^3.0.5",
    "vue-router": "^4.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "1.19.2",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-graphql-request": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-prettier-vue": "^2.1.1",
    "eslint-plugin-vue": "^7.8.0",
    "graphql-tag": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3",
    "vite": "^2.1.0",
    "vue-tsc": "^0.0.8"
  }
}
